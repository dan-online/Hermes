{
  "name": "Hermes",
  "version": "0.1.0",
  "main": "index.ts",
  "author": "DanCodes <dan@dancodes.online>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node index.ts",
    "dev:watch": "concurrently --kill-others \"yarn build:watch\" \"yarn start:nodemon\"",
    "dev:nodemon": "nodemon",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "copy-files": "cp -r src/assets dist/src/assets",
    "build": "yarn clean && tsc && yarn copy-files",
    "build:watch": "yarn copy-files && tsc -w",
    "start": "NODE_ENV=production node ./dist/index.js",
    "start:shards": "node ./dist/shard.js",
    "start:inspect": "node --inspect ./dist/index.js",
    "start:nodemon": "nodemon --signal SIGKILL --ext ts --exec yarn start"
  },
  "nodemonConfig": {
    "signal": "SIGKILL",
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public",
      "src/vault"
    ],
    "exec": "yarn dev"
  },
  "dependencies": {
    "@joshdb/core": "^1.2.6",
    "@joshdb/json": "https://gitpkg.now.sh/dan-online/josh-providers/josh-json?master",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "file-system": "^2.2.2",
    "jet-logger": "^1.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.4"
  },
  "optionalDependencies": {
    "jimp": "^0.16.1",
    "@tensorflow/tfjs-node-gpu": "^3.8.0",
    "axios": "^0.21.1",
    "@tensorflow/tfjs-node": "^3.8.0",
    "bufferutil": "^4.0.3",
    "canvas": "^2.8.0",
    "erlpack": "discord/erlpack",
    "nsfwjs": "^2.4.1",
    "utf-8-validate": "^5.0.5"
  }
}
